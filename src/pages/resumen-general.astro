---
import Layout from '../layouts/Layout.astro';
import PdfViewer from '../components/PdfViewer.astro';
import { getPdfUrl } from '../config/pdfs';

const pdfUrl = getPdfUrl('resumenGeneral');
---

<Layout title="Resumen General">
  <PdfViewer 
    title="Resumen General de Investigaciones"
    pdfUrl={pdfUrl}
    color="amber"
  />
</Layout>
