---
import Layout from '../layouts/Layout.astro';
import PdfViewer from '../components/PdfViewer.astro';
import { getPdfUrl } from '../config/pdfs';

const pdfUrl = getPdfUrl('arquitecturaEmpresaTi');
---

<Layout title="Arquitectura de una Empresa TI">
  <PdfViewer 
    title="Arquitectura de una Empresa TI"
    pdfUrl={pdfUrl}
    color="purple"
  />
</Layout>
