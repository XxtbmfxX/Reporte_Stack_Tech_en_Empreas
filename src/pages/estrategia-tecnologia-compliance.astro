---
import Layout from '../layouts/Layout.astro';
import PdfViewer from '../components/PdfViewer.astro';
import { getPdfUrl } from '../config/pdfs';

const pdfUrl = getPdfUrl('estrategiaTecnologiaCompliance');
---

<Layout title="Estrategia, Tecnología y Compliance">
  <PdfViewer 
    title="Estrategia, Tecnología y Compliance"
    pdfUrl={pdfUrl}
    color="orange"
  />
</Layout>
